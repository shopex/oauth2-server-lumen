{
    "name": "onex/oauth2-server-lumen",
    "description": "An OAuth 2.0 bridge for Laravel and Lumen",
    "keywords": ["oauth2", "lumen"],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Bryant Yan",
            "email": "bryant.yan@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "league/oauth2-server": "5.1.*",
        "illuminate/database": "5.1.* || 5.2.*",
        "illuminate/console": "5.1.* || 5.2.*",
        "illuminate/contracts": "5.1.* || 5.2.*",
        "illuminate/http": "5.1.* || 5.2.*",
        "illuminate/support": "5.1.* || 5.2.*",
        "illuminate/config": "5.1.* || 5.2.*",

        "symfony/psr-http-message-bridge": "0.*",
        "zendframework/zend-diactoros": "1.3.*"
    },
    "autoload": {
        "psr-4": {
            "Onex\\OAuth2Server\\": "src"
        }
    },
    "minimum-stability": "dev"
}

